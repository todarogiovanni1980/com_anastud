<?xml version="1.0" encoding="utf-8"?>
<form>
  <fieldset>
    <field name="id" type="text" readonly="true" class="readonly" default="0" description="JGLOBAL_FIELD_ID_DESC"/>
    <field name="ordering"/>
    <field name="state" default="1" type="list" label="JSTATUS" description="JFIELD_PUBLISHED_DESC" class="inputbox" size="1">
      <option value="1">JPUBLISHED</option>
      <option value="0">JUNPUBLISHED</option>
      <option value="2">JARCHIVED</option>
      <option value="-2">JTRASHED</option>
    </field>
    <field name="checked_out" type="hidden" filter="unset"/>
    <field name="checked_out_time" default="0000-00-00 00:00:00" type="hidden" filter="unset"/>
    <field name="created_by" type="createdby" default="0" label="COM_ANASTUD_FORM_LBL_STUDENTE_CREATED_BY" description="COM_ANASTUD_FORM_DESC_STUDENTE_CREATED_BY" hidden="true" hint="COM_ANASTUD_FORM_LBL_STUDENTE_CREATED_BY"/>
    <field name="modified_by" type="modifiedby" default="0" label="COM_ANASTUD_FORM_LBL_STUDENTE_MODIFIED_BY" description="COM_ANASTUD_FORM_DESC_STUDENTE_MODIFIED_BY" hidden="true" hint="COM_ANASTUD_FORM_LBL_STUDENTE_MODIFIED_BY"/>
    <field name="cognome" type="text" required="true" label="COM_ANASTUD_FORM_LBL_STUDENTE_COGNOME" description="COM_ANASTUD_FORM_DESC_STUDENTE_COGNOME" hint="COM_ANASTUD_FORM_LBL_STUDENTE_COGNOME"/>
    <field name="nome" type="text" label="COM_ANASTUD_FORM_LBL_STUDENTE_NOME" description="COM_ANASTUD_FORM_DESC_STUDENTE_NOME" hint="COM_ANASTUD_FORM_LBL_STUDENTE_NOME"/>
    <field name="matricola" type="text" label="COM_ANASTUD_FORM_LBL_STUDENTE_MATRICOLA" description="COM_ANASTUD_FORM_DESC_STUDENTE_MATRICOLA" hint="COM_ANASTUD_FORM_LBL_STUDENTE_MATRICOLA"/>
    <field name="foto" type="FileMultiple" label="COM_ANASTUD_FORM_LBL_STUDENTE_FOTO" description="COM_ANASTUD_FORM_DESC_STUDENTE_FOTO" hint="COM_ANASTUD_FORM_LBL_STUDENTE_FOTO"/>
    <field name="version_note" type="text" label="JGLOBAL_FIELD_VERSION_NOTE_LABEL" description="JGLOBAL_FIELD_VERSION_NOTE_DESC" class="inputbox" size="45" labelclass="control-label"/>
  </fieldset>
</form>
